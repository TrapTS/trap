{
  "name": "trap",
  "version": "1.0.0",
  "description": "Trap is base on koa, running in node.js v8 runtime.",
  "scripts": {
    "dev": "NODE_ENV=development nodemon --exec ts-node src/app.ts",
    "start": "NODE_ENV=release ts-node ./src/app.ts",
    "ws:dev": "NODE_ENV=development ts-node ./src/ws.ts",
    "rabbit:receive": "ts-node ./src/rabbitmq/receive.ts",
    "format": "prettier --write \"**/*.ts\"",
    "git-tag": "PACKAGE_VERSION=$(cat package.json | grep version | head -1 | awk -F: '{ print $2 }' | sed 's/[\",]//g' | tr -d '[[:space:]]') && git tag $PACKAGE_VERSION && git push --tags",
    "tsc": "tsc -p tsconfig.json"
  },
  "typings": "./src/typings/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TrapTS/trap.git"
  },
  "keywords": [
    "ts",
    "trap",
    "koa"
  ],
  "author": "AlfieriChou",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TrapTS/trap/issues"
  },
  "homepage": "https://github.com/TrapTS/trap#readme",
  "devDependencies": {
    "@types/amqplib": "0.5.9",
    "@types/cron": "1.3.0",
    "@types/ioredis": "4.0.6",
    "@types/knex": "0.15.1",
    "@types/koa": "2.0.48",
    "@types/koa-generic-session": "1.0.2",
    "@types/lodash": "4.14.120",
    "@types/node": "10.12.20",
    "nodemon": "1.18.9",
    "prettier": "1.16.2",
    "ts-node": "8.0.2",
    "tslint-config-prettier": "1.17.0",
    "tslint-plugin-prettier": "2.0.1"
  },
  "dependencies": {
    "amqplib": "0.5.3",
    "class-transformer": "0.2.0",
    "class-transformer-validator": "0.6.0",
    "class-validator": "0.9.1",
    "cron": "1.6.0",
    "dir_filenames": "1.0.6",
    "ioredis": "4.5.1",
    "knex": "0.16.3",
    "koa": "2.7.0",
    "koa-bodyparser": "4.2.1",
    "koa-generic-session": "2.0.1",
    "koa-logger": "3.2.0",
    "koa-redis": "3.1.3",
    "koa-router": "7.4.0",
    "lodash": "4.17.11",
    "mysql": "2.16.0",
    "node-cache": "4.2.0",
    "reflect-metadata": "0.1.13",
    "socket.io": "2.2.0",
    "typescript": "3.2.4"
  }
}
