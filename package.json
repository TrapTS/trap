{
  "name": "trap",
  "version": "1.0.0",
  "description": "Trap is base on koa, running in node.js v8 runtime.",
  "scripts": {
    "dev": "NODE_ENV=development nodemon --exec 'ts-node --cache-directory .cache' src/server.ts",
    "start": "NODE_ENV=release ts-node ./src/server.ts",
    "ws:dev": "NODE_ENV=development ts-node ./src/wsServer.ts",
    "rabbit:send": "ts-node ./src/rabbitmq/send.ts",
    "rabbit:receive": "ts-node ./src/rabbitmq/receive.ts",
    "format": "prettier --write \"**/*.ts\""
  },
  "typings": "./src/typings/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TrapTS/trap.git"
  },
  "keywords": [
    "ts",
    "trap",
    "koa"
  ],
  "author": "AlfieriChou",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TrapTS/trap/issues"
  },
  "homepage": "https://github.com/TrapTS/trap#readme",
  "devDependencies": {
    "@types/amqplib": "^0.5.9",
    "@types/cron": "^1.3.0",
    "@types/knex": "^0.15.1",
    "@types/koa": "^2.0.48",
    "@types/node": "^10.12.18",
    "nodemon": "^1.18.9",
    "prettier": "^1.15.3",
    "ts-node": "^7.0.1",
    "tslint-config-prettier": "^1.17.0",
    "tslint-plugin-prettier": "^2.0.1"
  },
  "dependencies": {
    "amqplib": "^0.5.3",
    "class-transformer": "^0.2.0",
    "cron": "^1.6.0",
    "dir_filenames": "^1.0.6",
    "inversify": "^5.0.1",
    "knex": "^0.16.3",
    "koa": "^2.6.2",
    "koa-bodyparser": "^4.2.1",
    "koa-logger": "^3.2.0",
    "koa-router": "^7.4.0",
    "lodash": "^4.17.11",
    "node-cache": "^4.2.0",
    "reflect-metadata": "^0.1.12",
    "socket.io": "^2.2.0",
    "typescript": "^3.2.2"
  }
}
